{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://pointline.dev/schemas/quant_research_output.v1.json",
  "title": "QuantResearchOutputV1",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_version",
    "request_id",
    "run",
    "assumptions",
    "data_feasibility",
    "experiment_spec",
    "quality_gates",
    "results",
    "decision"
  ],
  "properties": {
    "schema_version": {
      "const": "1.0"
    },
    "request_id": {
      "type": "string",
      "minLength": 3
    },
    "run": {
      "type": "object",
      "additionalProperties": false,
      "required": ["run_id", "started_at", "completed_at", "status"],
      "properties": {
        "run_id": { "type": "string", "minLength": 3 },
        "started_at": { "type": "string", "format": "date-time" },
        "completed_at": { "type": "string", "format": "date-time" },
        "status": { "type": "string", "enum": ["success", "partial", "failed"] }
      }
    },
    "assumptions": {
      "type": "array",
      "items": { "type": "string", "minLength": 1 }
    },
    "data_feasibility": {
      "type": "object",
      "additionalProperties": false,
      "required": ["coverage_checks", "probe_checks"],
      "properties": {
        "coverage_checks": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": ["table", "available"],
            "properties": {
              "table": {
                "type": "string",
                "enum": [
                  "trades",
                  "quotes",
                  "book_snapshot_25",
                  "derivative_ticker",
                  "kline_1h",
                  "szse_l3_orders",
                  "szse_l3_ticks"
                ]
              },
              "available": { "type": "boolean" },
              "reason": { "type": ["string", "null"] },
              "symbol_ids": {
                "type": ["array", "null"],
                "items": { "type": "integer" }
              }
            }
          }
        },
        "probe_checks": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": ["table", "window_start", "window_end", "row_count", "passed"],
            "properties": {
              "table": { "type": "string" },
              "window_start": { "type": "string" },
              "window_end": { "type": "string" },
              "row_count": { "type": "integer", "minimum": 0 },
              "passed": { "type": "boolean" }
            }
          }
        }
      }
    },
    "experiment_spec": {
      "type": "object",
      "additionalProperties": false,
      "required": ["hypothesis", "features", "joins", "label_definition", "evaluation_plan"],
      "properties": {
        "hypothesis": { "type": "string", "minLength": 5 },
        "features": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": ["name", "definition", "source_tables"],
            "properties": {
              "name": { "type": "string", "minLength": 1 },
              "definition": { "type": "string", "minLength": 1 },
              "source_tables": {
                "type": "array",
                "minItems": 1,
                "items": { "type": "string" },
                "uniqueItems": true
              }
            }
          }
        },
        "joins": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": ["left", "right", "on", "strategy"],
            "properties": {
              "left": { "type": "string" },
              "right": { "type": "string" },
              "on": { "type": "string" },
              "strategy": { "type": "string", "enum": ["backward", "forward", "exact"] },
              "pit_safe": { "type": "boolean", "default": true }
            }
          }
        },
        "label_definition": {
          "type": "string",
          "minLength": 1
        },
        "evaluation_plan": {
          "type": "string",
          "minLength": 1
        }
      }
    },
    "quality_gates": {
      "type": "object",
      "additionalProperties": false,
      "required": ["lookahead_check", "pit_ordering_check", "reproducibility_check"],
      "properties": {
        "lookahead_check": {
          "type": "object",
          "additionalProperties": false,
          "required": ["passed", "evidence"],
          "properties": {
            "passed": { "type": "boolean" },
            "evidence": { "type": "string", "minLength": 1 }
          }
        },
        "pit_ordering_check": {
          "type": "object",
          "additionalProperties": false,
          "required": ["passed", "timeline"],
          "properties": {
            "passed": { "type": "boolean" },
            "timeline": { "type": "string", "enum": ["ts_local_us", "ts_exch_us"] }
          }
        },
        "reproducibility_check": {
          "type": "object",
          "additionalProperties": false,
          "required": ["passed", "input_hash"],
          "properties": {
            "passed": { "type": "boolean" },
            "input_hash": { "type": "string", "minLength": 8 }
          }
        },
        "failed_gates": {
          "type": "array",
          "items": { "type": "string" }
        }
      }
    },
    "results": {
      "type": "object",
      "additionalProperties": false,
      "required": ["metrics", "regime_results", "interpretation"],
      "properties": {
        "metrics": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": ["name", "split", "value"],
            "properties": {
              "name": { "type": "string" },
              "split": { "type": "string", "enum": ["train", "validation", "test", "oos"] },
              "value": { "type": "number" },
              "ci_lower": { "type": ["number", "null"] },
              "ci_upper": { "type": ["number", "null"] }
            }
          }
        },
        "regime_results": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": ["regime", "summary"],
            "properties": {
              "regime": { "type": "string" },
              "summary": { "type": "string" }
            }
          }
        },
        "interpretation": {
          "type": "object",
          "additionalProperties": false,
          "required": ["facts", "analysis"],
          "properties": {
            "facts": { "type": "array", "items": { "type": "string" } },
            "analysis": { "type": "array", "items": { "type": "string" } }
          }
        }
      }
    },
    "decision": {
      "type": "object",
      "additionalProperties": false,
      "required": ["status", "rationale", "next_actions"],
      "properties": {
        "status": { "type": "string", "enum": ["go", "revise", "reject"] },
        "rationale": { "type": "string", "minLength": 1 },
        "risks": { "type": "array", "items": { "type": "string" } },
        "next_actions": {
          "type": "array",
          "items": { "type": "string", "minLength": 1 }
        }
      }
    },
    "artifacts": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "paths": {
          "type": "array",
          "items": { "type": "string" }
        }
      }
    }
  }
}
